{% extends 'common/base.html.j2' %}

{% block content %}
  <h3>My Experiments</h3>
  {% for experiment in experiments %}
    <div class="list-result">
      <h4><a href="{{ url_for('.show_experiment', experiment_uuid_hex=experiment.get_pk().hex) }}">
        {{ experiment.name }}
      </a></h4>
      <p>
        {{ experiment.description }}
      </p>
    </div>
  {% endfor %}
{% endblock %}
